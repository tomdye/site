{
	"build-app": {
		"build-time-render": {
			"root": "app",
			"static": true,
			"renderer": "jsdom",
			"paths": [
				"contact-thank-you",
				"error-404",
				{
					"path": "blog/search",
					"static": false
				},
				{
					"path": "contact",
					"static": false
				},
				{
					"path": "rss.xml",
					"exclude": true
				}
			]
		},
		"pwa": {
			"manifest": {
				"name": "sitepen.com",
				"short_name": "sitepen.com",
				"description": "sitepen.com",
				"background_color": "#ffffff",
				"theme_color": "#5cdbdd",
				"icons": [
					{
						"src": "src/favicon.png",
						"sizes": [
							16,
							192,
							512
						]
					}
				]
			}
		}
	},
	"create-app": {}
}
